{% extends "BeTeenIndexBundle:Layout:Global.html.twig" %}
{% block header %}
<link rel="stylesheet" href="{{asset("css/profil.css")}}" />
{% endblock %}
{% block body %}
<div class="profilBox" style="background-image: url('{{asset("uploads/back/"~utilisateur.background)}}');">
    <div class="content">
        <img class="avatar" src="{{asset("uploads/avatar/"~utilisateur.avatar)}}" alt="Avatar de {{utilisateur.username}}" />
        {% block content %}{% endblock %}
    </div>
</div>
{% endblock %}